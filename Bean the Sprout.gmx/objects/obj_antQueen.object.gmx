          <argument>
            <kind>1</kind>
            <string>///Antie Queen Controls
hsp = dir * movespeed;
vsp += grav;
image_speed = 0.4;

//Gravity
vsp += grav;

//Horizontal Collisions for wall only
if(place_meeting(x + hsp, y, obj_ground1)){
    while(!place_meeting( x + sign(hsp), y, obj_ground1)) x += sign(hsp);
    hsp=0;
    dir *= -1;
}

// Horizontal Collision for monsterStop Object
if (place_meeting(x + hsp, y, obj_monsterStop)) {
    dir *= -1;
}


//Vertical Collisions
if(place_meeting(x, y + vsp, obj_ground1)){
    while(!place_meeting(x, y + sign(vsp), obj_ground1)) y += sign(vsp);
    //if(sign(vsp) == 1){ grounded = true; }
    vsp = 0;
    
    if((fearofheights) &amp;&amp; (!position_meeting( x + (sprite_width /2 ) * dir, y + (sprite_height / 2) + 8, obj_ground1))){
        dir *= -1;
    }
}

//Antie Collision with Bean
if(place_meeting(x, y, obj_beanMain)){
    if(obj_beanMain.y &lt; y - 16){ // jump on top of the ant
        effect_create_above(ef_explosion, x, y, 1, c_green);
        antHealth -= 20 ;
        health -= 10;
    } else {
        audio_play_sound(snd_antBite, 0, 0);
        if obj_beanMain.alarm[0] &lt; 0 //Then check if the alarm isn't already counting down (when an alarm is off, it's value is -1)
        {
        health -= 30;
        obj_beanMain.image_blend = c_red; //Then give this object a different blended color
        obj_beanMain.alarm[0] = room_speed; //And set the alarm to trigger after one second.
        }
        knockback = true;
        movespeed = -15; // knockback effect, set speed in reverse
        alarm[1] = 5;   // set the alarm
        
        x += hsp;
    }
}

if(place_meeting(x, y, obj_leafBullet)){
    antHealth -= 20 ;
    }

if(antHealth &lt;= 0){
    instance_destroy();
    score += 500
    room_goto(rm_youWin);
}


//Finalize Horizontal
x += hsp;

//Finalize Vertical
y += vsp;
</string>
